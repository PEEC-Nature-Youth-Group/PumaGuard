{
	"name": "PumaGuard Container",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"USER_UID": "1000",
			"USER_GID": "1000",
			"http_proxy": "${localEnv:http_proxy}",
			"https_proxy": "${localEnv:https_proxy}"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	}
}
